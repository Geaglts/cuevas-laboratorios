<Files .htaccess>
Order deny,allow
Deny from all
</Files>

RewriteEngine On
RewriteBase /
RewriteCond %{HTTP_HOST} ^www\.(.*)$ [NC]
RewriteRule ^(.*)$ https://%1/$1 [R=301,L]

RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)/$ /$1 [R=301,L]

RewriteEngine on
RewriteCond %{HTTP_HOST} ^https://laboratorioscuevas.com.mx/pages/blog.html[NC,OR]
RewriteRule ^(.*)$ http://laboratorioscuevas.com.mx/blog/$1 [L,R=301,NC]